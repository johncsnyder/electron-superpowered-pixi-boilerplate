{
  "name": "electron-superpowered-pixi-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for Electron integrated with Superpowered Audio and WebGL",
  "main": "main.js",
  "devDependencies": {
    "electron-connect": "0.6.1",
    "gulp": "3.9.1"
  },
  "scripts": {
    "start": "NODE_ENV=DEVELOPMENT gulp serve",
    "build:osx": "electron-packager ./ --electron-version=1.6.11 --extra-resource=build/Release/filter.node --platform=darwin --arch=x64 --overwrite --prune=true --out build/"
